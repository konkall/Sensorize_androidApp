<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="@dimen/activity_vertical_margin"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
tools:context="com.collisiondetector.project_anaptujh.Settings.Main2Activity" >

<SeekBar
    android:id="@+id/frequency_seekBar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:max="1000"
    android:paddingTop="0dp"
    android:layout_below="@+id/light"
    android:layout_alignParentStart="true"
    android:layout_marginTop="21dp" />


<TextView
    android:id="@+id/light"
    android:layout_height="wrap_content"
    android:paddingTop="10dp"
    android:paddingLeft="10dp"
    android:text="FREQUENCY"
    android:layout_alignParentTop="true"
    android:layout_alignStart="@+id/gravity"
    android:layout_width="wrap_content"
    android:textStyle="bold|italic" />


<TextView
    android:id="@+id/textfrequency"
    android:text=""
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/light"
    android:layout_centerHorizontal="true" />





<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:orientation="horizontal"
    android:layout_marginTop="14dp"
    android:layout_below="@+id/frequency_seekBar"
    android:layout_alignParentStart="true">
    <TextView
        android:id="@+id/label1"
        android:paddingLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="20" />
    <TextView
        android:id="@+id/Label2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="490" />

    <TextView
        android:id="@+id/Label3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingRight="10dp"
        android:text="1000"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>



<SeekBar
    android:id="@+id/sensivity_seekBar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:max="20"
    android:paddingTop="0dp"
    android:layout_marginTop="22dp"
    android:layout_below="@+id/gravity"
    android:layout_alignParentStart="true" />



<TextView
    android:id="@+id/gravity"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingTop="30dp"
    android:paddingLeft="10dp"
    android:text="SENSIVITY"
    android:layout_alignParentStart="true"
    android:layout_marginTop="99dp"
    android:textStyle="bold|italic"/>


<TextView
    android:id="@+id/textsensivity"
    android:text=""
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/gravity"
    android:layout_alignEnd="@+id/textfrequency"
    android:layout_centerHorizontal="true" />



<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:orientation="horizontal"
    android:layout_below="@+id/sensivity_seekBar"
    android:layout_alignParentStart="true"
    android:layout_marginTop="12dp">
    <TextView
        android:id="@+id/Label4"
        android:paddingLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.75" />
    <TextView
        android:id="@+id/Label5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="" />
    <TextView
        android:id="@+id/Label6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:paddingRight="10dp"
        android:text="0.95" />
</RelativeLayout>





</RelativeLayout>


