<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/activity_main"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="@dimen/activity_vertical_margin"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
tools:context="com.collisiondetector.project_anaptujh.MainActivity"
android:background="@drawable/back">


<TextView
    android:id="@+id/LIGHT_available"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"/>


<TextView
    android:id="@+id/ACC_available"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"
    android:layout_below="@+id/LIGHT_available"
    android:layout_alignParentStart="true" />

<TextView
    android:id="@+id/GRAV_available"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"
    android:layout_below="@+id/ACC_available"
    android:layout_alignParentStart="true" />

    <TextView
    android:id="@+id/switchtext"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    android:text=""
    android:textColor="#FFFFFF"
    android:layout_marginBottom="27dp"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true" />

<TextView
    android:id="@+id/LIGHT_reading"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"
    android:layout_marginTop="51dp"
    android:layout_below="@+id/GRAV_available"
    android:layout_alignParentStart="true" />

<TextView
    android:id="@+id/ACC_reading"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"
    android:layout_below="@+id/LIGHT_reading"
    android:layout_toStartOf="@+id/mySwitch"
    android:layout_marginTop="28dp"
    android:layout_alignParentEnd="true" />

<TextView
    android:id="@+id/GRAVITY_reading"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"
    android:layout_below="@+id/ACC_reading"
    android:layout_alignEnd="@+id/ACC_reading"
    android:layout_marginTop="21dp" />


<TextView
    android:id="@+id/aa"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"
    android:layout_marginTop="63dp"
    android:layout_below="@+id/GRAVITY_reading"
    android:layout_alignParentStart="true" />

<TextView
    android:id="@+id/bb"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:textColor="#FFFFFF"
    android:layout_below="@+id/aa"
    android:layout_alignParentStart="true"
    android:layout_marginTop="21dp" />

    <Switch
        android:id="@+id/mySwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:thumb="@drawable/cust_switch"
        android:layout_above="@+id/switchtext"
        android:layout_alignStart="@+id/myOnlineSwitch" />

    <TextView
        android:id="@+id/switchOnlinetext"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text=""
        android:textColor="#FFFFFF"
        android:layout_marginBottom="14dp"
        android:layout_above="@+id/mySwitch"
        android:layout_alignStart="@+id/switchtext" />

    <Switch
        android:id="@+id/myOnlineSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:thumb="@drawable/cust_switch"
        android:layout_above="@+id/switchOnlinetext"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
